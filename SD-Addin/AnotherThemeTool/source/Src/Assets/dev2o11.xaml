<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:ad="clr-namespace:AvalonDock;assembly=AvalonDock" xmlns:adRes="clr-namespace:AvalonDock.Properties;assembly=AvalonDock">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/AvalonDock;component/themes/generic.xaml" />
		<ResourceDictionary Source="/Mu;component/src/assets/toolbar.xaml" />
		<ResourceDictionary Source="/Mu;component/src/assets/menuitems-default.xaml" />
	</ResourceDictionary.MergedDictionaries>
	<DrawingBrush x:Key="SeePreviousBrush" Stretch="Uniform" Viewbox="0,0,3,3" Viewport="0,0,3,3" ViewportUnits="Absolute" ViewboxUnits="Absolute" AlignmentX="Left" AlignmentY="Top" TileMode="Tile">
		<DrawingBrush.Drawing>
			<DrawingGroup>
				<GeometryDrawing Brush="#FF0A246A">
					<GeometryDrawing.Geometry>
						<RectangleGeometry Rect="0,0,3,3" />
					</GeometryDrawing.Geometry>
				</GeometryDrawing>
				<GeometryDrawing Brush="#FF293955">
					<GeometryDrawing.Geometry>
						<GeometryGroup>
							<RectangleGeometry Rect="0,0,1,1" />
						</GeometryGroup>
					</GeometryDrawing.Geometry>
				</GeometryDrawing>
			</DrawingGroup>
		</DrawingBrush.Drawing>
	</DrawingBrush>
	<SolidColorBrush x:Key="LightForegroundBrush" Color="Black" />
	<SolidColorBrush x:Key="DarkForegroundBrush" Color="White" />
	<SolidColorBrush x:Key="ManagedContentTabControlNormalBorderBrush" Color="#FF365697" />
	<SolidColorBrush x:Key="ManagedContentTabItemNormalBackground" Color="#000A246A" />
	<SolidColorBrush x:Key="ManagedContentTabItemSelectedBorderBrush" Color="#FF0A246A" />
	<SolidColorBrush x:Key="ManagedContentTabItemSelectedBackground" Color="#FF0A246A" />
	
	<ImageBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DefaultBackgroundBrush}, TypeInTargetAssembly={x:Type ad:DockingManager}}" />
	
	<LinearGradientBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DockablePaneTitleBackground}, TypeInTargetAssembly={x:Type ad:DockingManager}}" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#4D6082" Offset="0" />
		<GradientStop Color="#3D5277" Offset="1" />
	</LinearGradientBrush>
	
	<LinearGradientBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DockablePaneTitleBackgroundSelected}, TypeInTargetAssembly={x:Type ad:DockingManager}}" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#FFFCF2" Offset="0" />
		<GradientStop Color="#FFEDBA" Offset="0.5" />
		<GradientStop Color="#FFE8A6" Offset="0.5" />
		<GradientStop Color="#FFE8A6" Offset="1" />
	</LinearGradientBrush>
	
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DockablePaneTitleForeground}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="White" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DockablePaneTitleForegroundSelected}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="Black" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBackground}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF0A246A" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundMouseOver}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF858585" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundSelected}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF0A246A" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundSelectedActivated}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF0A246A" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBorder}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF0A246A" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBorderSelected}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF0A246A" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBorderBrushMouseOver}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF071A4D" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderBorderSelectedActivated}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FFE8A6" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderForeground}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="#FF373737" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderForegroundSelected}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="Black" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.DocumentHeaderForegroundSelectedActivated}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="Black" />
	<SolidColorBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.PaneHeaderCommandBackground}, TypeInTargetAssembly={x:Type ad:DockingManager}}" Color="White" />
	
	<LinearGradientBrush x:Key="{ComponentResourceKey ResourceId={x:Static ad:AvalonDockBrushes.PaneHeaderCommandBorderBrush}, TypeInTargetAssembly={x:Type ad:DockingManager}}" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#FFFFE8A6" Offset="0" />
		<GradientStop Color="#FFFFE8A6" Offset="0.5" />
		<GradientStop Color="#FFFFEDBA" Offset="0.5" />
		<GradientStop Color="#FFFFFCF2" Offset="1" />
	</LinearGradientBrush>
	
	<SolidColorBrush x:Key="ManagedContentTabItemInvNormalBackground" Color="White" />
	
	<SolidColorBrush x:Key="ManagedContentTabItemHotBackground" Color="#FF858585" />
	
	<LinearGradientBrush x:Key="ManagedContentTabItemInvHotBackground" StartPoint="0,1" EndPoint="0,0">
		<LinearGradientBrush.GradientStops>
			<GradientStop Color="#FFFFFCF2" />
			<GradientStop Color="#FFFFEDBA" Offset="0.5" />
			<GradientStop Color="#FFFFE8A6" Offset="0.5" />
			<GradientStop Color="#FFFFA6A6" Offset="1" />
		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>
	
	<SolidColorBrush x:Key="ManagedContentTabItemDisabledBackground" Color="#FF8D8D8D" />
	
	<SolidColorBrush x:Key="ManagedContentTabItemSelectedBorderBackround" Color="#FF313131" />
	
	<SolidColorBrush x:Key="ManagedContentTabItemNormalBorderBrush" Color="#190A246A" />
	
	<SolidColorBrush x:Key="ManagedContentTabItemHotBorderBrush" Color="#FF0A246A" />
	
	<SolidColorBrush x:Key="ManagedContentTabItemDisabledBorderBrush" Color="#54000000" />
	
	<SolidColorBrush x:Key="OverlayWindowMainBorderBrush" Color="#636871" />
	
	<SolidColorBrush x:Key="OverlayWindowIntBorderBrush" Color="#FF0A246A" />
	
	<LinearGradientBrush x:Key="OverlayWindowIntBorderBackground" StartPoint="0,0" EndPoint="0,1">
		<LinearGradientBrush.GradientStops>
			<GradientStop Color="#F8FAFC" Offset="0" />
			<GradientStop Color="#E8EBF0" Offset="1" />
		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>
	<SolidColorBrush x:Key="OverlayWindowIntBorderBrush2" Color="#7C8AA1" />
	<LinearGradientBrush x:Key="OverlayWindowIntBorderBackground2" StartPoint="0,0" EndPoint="0,1">
		<LinearGradientBrush.GradientStops>
			<GradientStop Color="#FEEEC0" Offset="0" />
			<GradientStop Color="#F9D79C" Offset="1" />
		</LinearGradientBrush.GradientStops>
	</LinearGradientBrush>
	<SolidColorBrush x:Key="DocumentFloatingWindowExtBorderSelectedBrush" Color="#C0A776" />
	
	<Style x:Key="{x:Type ad:DockingManager}" TargetType="{x:Type ad:DockingManager}">
		<Setter Property="Control.Background" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DefaultBackgroundBrush}}}" />
		<Setter Property="FrameworkElement.FocusVisualStyle" Value="{x:Null}" />
		<Setter Property="Control.Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:DockingManager}">
					<Grid Background="{TemplateBinding Control.Background}">
						<Grid.ColumnDefinitions>
							<ColumnDefinition Width="Auto" />
							<ColumnDefinition />
							<ColumnDefinition Width="Auto" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition Height="Auto" />
							<RowDefinition />
							<RowDefinition Height="Auto" />
						</Grid.RowDefinitions>
						<StackPanel Name="PART_LeftAnchorTabPanel" Grid.Column="0" Grid.Row="1" Orientation="Vertical">
							<StackPanel.Resources>
								<Style TargetType="{x:Type ad:DockablePaneAnchorTabGroup}">
									<Setter Property="FrameworkElement.LayoutTransform">
										<Setter.Value>
											<RotateTransform Angle="90" />
										</Setter.Value>
									</Setter>
									<Setter Property="FrameworkElement.Margin" Value="0,2,2,0" />
								</Style>
								<Style TargetType="{x:Type Border}">
									<Setter Property="Border.CornerRadius" Value="0,0,3,3" />
								</Style>
							</StackPanel.Resources>
						</StackPanel>
						<StackPanel Name="PART_RightAnchorTabPanel" Grid.Column="2" Grid.Row="1" Orientation="Vertical">
							<StackPanel.Resources>
								<Style TargetType="{x:Type ad:DockablePaneAnchorTabGroup}">
									<Setter Property="FrameworkElement.LayoutTransform">
										<Setter.Value>
											<RotateTransform Angle="90" />
										</Setter.Value>
									</Setter>
									<Setter Property="FrameworkElement.Margin" Value="2,2,0,0" />
								</Style>
								<Style TargetType="{x:Type Border}">
									<Setter Property="Border.CornerRadius" Value="3,3,0,0" />
								</Style>
							</StackPanel.Resources>
						</StackPanel>
						<StackPanel Name="PART_TopAnchorTabPanel" Grid.Column="1" Grid.Row="0" Orientation="Horizontal" />
						<StackPanel Name="PART_BottomAnchorTabPanel" Grid.Column="1" Grid.Row="2" Orientation="Horizontal" />
						<Border Name="PART_InternalContainer" Background="{TemplateBinding Control.Background}" Grid.Column="1" Grid.Row="1" Padding="4">
							<ContentPresenter />
						</Border>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style x:Key="DockableContentTabItemStyle" TargetType="{x:Type ad:DockableContent}">
		<Setter Property="Control.Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:DockableContent}">
					<Grid SnapsToDevicePixels="True">
						<Border Name="PART_DragArea" BorderBrush="{StaticResource ManagedContentTabControlNormalBorderBrush}" BorderThickness="1,0,1,1" Margin="0,0,0,0" Background="{StaticResource ManagedContentTabItemNormalBackground}" CornerRadius="0,0,2,2" Padding="1">
							<Grid Margin="4,0,4,0">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="*" />
								</Grid.ColumnDefinitions>
								<ad:ImageEx Name="Icon" FrameworkElement.Width="16" Image.Source="{TemplateBinding ad:ManagedContent.Icon}" />
								<TextBlock Name="tabItemTitle" Grid.Column="1" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" Text="{TemplateBinding ad:ManagedContent.Title}" Margin="2,0,0,0" VerticalAlignment="Center" TextBlock.Foreground="{StaticResource LightForegroundBrush}" />
							</Grid>
						</Border>
					</Grid>
					<ControlTemplate.Triggers>
						<Trigger Property="Selector.IsSelected" Value="True">
							<Setter Value="{StaticResource ManagedContentTabItemSelectedBorderBrush}" TargetName="PART_DragArea" Property="Border.BorderBrush" />
							<Setter Value="{StaticResource ManagedContentTabItemSelectedBackground}" TargetName="PART_DragArea" Property="Border.Background" />
							<Setter Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" TargetName="tabItemTitle" Property="TextBlock.Foreground" />
							<Setter Property="Panel.ZIndex" Value="100" />
						</Trigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="UIElement.IsMouseOver" Value="True" />
								<Condition Property="Selector.IsSelected" Value="False" />
							</MultiTrigger.Conditions>
							<Setter Value="{StaticResource ManagedContentTabItemHotBorderBrush}" TargetName="PART_DragArea" Property="Border.BorderBrush" />
							<Setter Value="{StaticResource ManagedContentTabItemHotBackground}" TargetName="PART_DragArea" Property="Border.Background" />
							<Setter Property="Panel.ZIndex" Value="99" />
						</MultiTrigger>
						<Trigger Property="UIElement.IsEnabled" Value="False">
							<Setter Value="{StaticResource ManagedContentTabItemDisabledBorderBrush}" TargetName="PART_DragArea" Property="Border.BorderBrush" />
							<Setter Value="{StaticResource ManagedContentTabItemDisabledBackground}" TargetName="PART_DragArea" Property="Border.Background" />
							<Setter Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}" TargetName="tabItemTitle" Property="TextBlock.Foreground" />
							<Setter TargetName="Icon" Property="UIElement.IsEnabled" Value="False" />
						</Trigger>
						<Trigger Value="{x:Null}" Property="ad:ManagedContent.Icon">
							<Setter TargetName="Icon" Property="UIElement.Visibility" Value="Collapsed" />
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style x:Key="{x:Type ad:DockablePane}" TargetType="{x:Type ad:DockablePane}">
		<Setter Property="Control.Background" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DefaultBackgroundBrush}}}" />
		<Setter Property="Control.Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:DockablePane}">
					<Border Focusable="False" Background="{TemplateBinding Control.Background}">
						<Grid FocusManager.FocusedElement="{Binding ElementName=PART_SelectedContent}">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" />
								<RowDefinition Height="Auto" />
							</Grid.RowDefinitions>
							<Border Name="PART_Header" Grid.Row="0" Focusable="False" CornerRadius="2,2,0,0" Height="18" Background="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DockablePaneTitleBackground}}}">
								<DockPanel LastChildFill="True">
									<Button Name="btnClose" DockPanel.Dock="Right" Style="{StaticResource PaneHeaderCommandStyle}" Command="ad:ManagedContentCommands.Close" ToolTip="{x:Static adRes:Resources.DockablePaneCommands_Close}" CommandTarget="{TemplateBinding Selector.SelectedItem}" Width="15" Height="15" Margin="2,0,2,0" VerticalAlignment="Center" Opacity="0.6">
										<Image Source="/AvalonDock;component/resources/Images/PinClose.png" Width="13" Height="13" Stretch="None" />
									</Button>
									<Button Name="btnPinAutoHide" ToolTip="{x:Static adRes:Resources.DockablePaneCommands_ToggleAutoHide}" DockPanel.Dock="Right" Style="{StaticResource PaneHeaderCommandStyle}" Command="ad:DockablePaneCommands.ToggleAutoHide" Width="15" Height="15" VerticalAlignment="Center" Opacity="0.6">
										<Image Source="/AvalonDock;component/resources/Images/PinAutoHide.png" Width="13" Height="13" Stretch="None" />
									</Button>
									<Border Name="PART_ShowContextMenuButton" DockPanel.Dock="Right" ToolTip="{x:Static adRes:Resources.DockablePane_ShowOptionsMenu}" Style="{StaticResource PaneHeaderContextMenuCommandStyle}" Width="15" Height="15" VerticalAlignment="Center" Opacity="0.6">
										<Image Source="/AvalonDock;component/resources/Images/PinMenu.png" Width="13" Height="13" Stretch="None" />
									</Border>
									<TextBlock Name="paneTitle" Grid.Row="0" Margin="2,0,0,0" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" VerticalAlignment="Center" Text="{Binding SelectedItem.Title, RelativeSource={RelativeSource TemplatedParent}}" TextBlock.Foreground="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DockablePaneTitleForeground}}}" />
								</DockPanel>
							</Border>
							<Border Grid.Row="1" Background="{Binding SelectedItem.Background, RelativeSource={RelativeSource TemplatedParent}}">
								<ContentPresenter Name="PART_SelectedContent" KeyboardNavigation.TabNavigation="Local" KeyboardNavigation.DirectionalNavigation="Contained" KeyboardNavigation.TabIndex="1" Content="{Binding SelectedItem.Content, RelativeSource={RelativeSource TemplatedParent}}" />
							</Border>
							<Border Name="PART_Tabs" Grid.Row="2" Height="23" BorderThickness="0,1,0,0" BorderBrush="{StaticResource ManagedContentTabItemSelectedBackground}">
								<ad:DockableTabPanel Panel.IsItemsHost="True" TabItemStyle="{DynamicResource DockableContentTabItemStyle}" />
							</Border>
						</Grid>
					</Border>
					<ControlTemplate.Triggers>
						<DataTrigger Value="True" Binding="{Binding SelectedItem.IsActiveContent, RelativeSource={RelativeSource Self}}">
							<Setter TargetName="PART_Header" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DockablePaneTitleBackgroundSelected}}}" Property="Border.Background" />
							<Setter TargetName="paneTitle" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DockablePaneTitleForegroundSelected}}}" Property="TextBlock.Foreground" />
						</DataTrigger>
						<Trigger Property="ad:Pane.ShowHeader" Value="False">
							<Setter TargetName="PART_Header" Property="UIElement.Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="ad:DockablePane.ShowTabs" Value="False">
							<Setter TargetName="PART_Tabs" Property="UIElement.Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="ad:Pane.HasSingleItem" Value="True">
							<Setter TargetName="PART_Tabs" Property="UIElement.Visibility" Value="Collapsed" />
						</Trigger>
						<DataTrigger Value="false" Binding="{Binding SelectedItem.IsCloseable, RelativeSource={RelativeSource Self}}">
							<Setter TargetName="btnClose" Property="UIElement.Visibility" Value="Collapsed" />
						</DataTrigger>
						<DataTrigger Value="AutoHide" Binding="{Binding SelectedItem.State, RelativeSource={RelativeSource Self}}">
							<Setter TargetName="btnPinAutoHide" Property="FrameworkElement.LayoutTransform">
								<Setter.Value>
									<RotateTransform Angle="90" />
								</Setter.Value>
							</Setter>
						</DataTrigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style x:Key="DocumentTabItemStyle" TargetType="{x:Type ad:ManagedContent}">
		<Setter Property="Control.Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:ManagedContent}">
					<Border Name="tabItemIntBorder" Grid.Column="1" BorderThickness="1,1,1,0" Padding="5,0,5,4" Margin="-1,0,0,-4" CornerRadius="2,2,0,0" Background="#00FFFFFF">
						<StackPanel Name="PART_DragArea" Orientation="Horizontal" Background="#00FFFFFF">
							<Image Source="{TemplateBinding ad:ManagedContent.Icon}" Width="0" />
							<TextBlock Name="tabItemTitle" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" Text="{TemplateBinding ad:ManagedContent.Title}" TextBlock.Foreground="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderForeground}}}" />
							<ad:ImageEx Name="PART_LockedIcon" FrameworkElement.Margin="2,0,0,0" UIElement.Visibility="Collapsed" FrameworkElement.VerticalAlignment="Center" FrameworkElement.HorizontalAlignment="Center" Image.Source="/AvalonDock;component/resources/Images/Locked.png" FrameworkElement.Width="6" />
							<Button Name="PART_CloseButton" Visibility="Hidden" Margin="6,2,2,2" Style="{StaticResource PaneHeaderCommandStyle}" ToolTip="{x:Static adRes:Resources.ManagedContentCommands_Close}" Command="ad:ManagedContentCommands.Close" Opacity="0.8">
								<ad:ImageEx Image.Source="/AvalonDock;component/resources/Images/PinClose.png" FrameworkElement.VerticalAlignment="Center" FrameworkElement.HorizontalAlignment="Center" FrameworkElement.Width="13" />
							</Button>
						</StackPanel>
					</Border>
					<ControlTemplate.Triggers>
						<Trigger Property="Selector.IsSelected" Value="True">
							<Setter TargetName="tabItemIntBorder" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBackground}}}" Property="Border.Background" />
							<Setter TargetName="PART_CloseButton" Property="UIElement.Visibility" Value="Visible" />
						</Trigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="ad:ManagedContent.IsActiveDocument" Value="True" />
								<Condition Property="ad:ManagedContent.IsActiveContent" Value="True" />
							</MultiTrigger.Conditions>
							<Setter TargetName="tabItemIntBorder" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundSelectedActivated}}}" Property="Border.Background" />
							<Setter TargetName="tabItemTitle" Value="{StaticResource DarkForegroundBrush}" Property="TextBlock.Foreground" />
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition Property="ad:ManagedContent.IsActiveDocument" Value="True" />
								<Condition Property="ad:ManagedContent.IsActiveContent" Value="False" />
							</MultiTrigger.Conditions>
							<Setter TargetName="tabItemIntBorder" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundSelected}}}" Property="Border.Background" />
							<Setter TargetName="tabItemTitle" Value="{StaticResource DarkForegroundBrush}" Property="TextBlock.Foreground" />
						</MultiTrigger>
						<MultiTrigger>
							<MultiTrigger.Conditions>
								<Condition SourceName="tabItemIntBorder" Property="UIElement.IsMouseOver" Value="True" />
								<Condition Property="Selector.IsSelected" Value="False" />
							</MultiTrigger.Conditions>
							<Setter TargetName="tabItemIntBorder" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBackgroundMouseOver}}}" Property="Border.Background" />
							<Setter TargetName="tabItemIntBorder" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBorderBrushMouseOver}}}" Property="Border.BorderBrush" />
							<Setter TargetName="PART_CloseButton" Property="UIElement.Visibility" Value="Visible" />
						</MultiTrigger>
						<Trigger Property="ad:ManagedContent.IsLocked" Value="True">
							<Setter TargetName="PART_LockedIcon" Property="UIElement.Visibility" Value="Visible" />
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>
	<Style x:Key="{x:Type ad:DocumentPane}" TargetType="{x:Type ad:DocumentPane}">
		<Setter Property="Control.Background" Value="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DefaultBackgroundBrush}}}" />
		<Setter Property="FrameworkElement.FocusVisualStyle" Value="{x:Null}" />
		<Setter Property="Control.Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type ad:DocumentPane}">
					<ControlTemplate.Resources>
						<ContextMenu x:Key="DocumentsListMenu" StaysOpen="True" />
					</ControlTemplate.Resources>
					<Border Background="{TemplateBinding Control.Background}">
						<Grid>
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto" />
								<RowDefinition Height="*" />
							</Grid.RowDefinitions>
							<Border Name="PART_Header" Grid.Row="0" Focusable="False">
								<Grid>
									<Grid.ColumnDefinitions>
										<ColumnDefinition Width="*" />
										<ColumnDefinition Width="18" />
									</Grid.ColumnDefinitions>
									<ad:DocumentTabPanel Name="paneTabsPanel" Panel.ZIndex="1" Panel.IsItemsHost="True" TabItemStyle="{StaticResource DocumentTabItemStyle}" />
									<Button Name="PART_ShowContextMenuButton" Grid.Column="1" Width="15" Height="15" Style="{StaticResource PaneHeaderCommandStyle}">
										<Image Name="ShowContextMenuIcon" Source="/AvalonDock;component/resources/Images/PinMenu.png" Width="13" Height="13" Stretch="None" />
									</Button>
								</Grid>
							</Border>
							<Grid Grid.Row="1">
								<Border Name="topBorder" Height="4" CornerRadius="2,2,0,0" VerticalAlignment="Top" HorizontalAlignment="Stretch" Background="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBorder}}}" />
								<Border Name="bottomBorder" Height="4" CornerRadius="0,0,2,2" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" Background="{DynamicResource {ComponentResourceKey {x:Type ad:DockingManager}, {x:Static ad:AvalonDockBrushes.DocumentHeaderBorder}}}" />
								<ContentPresenter Margin="0,4,0,4" KeyboardNavigation.TabNavigation="Local" KeyboardNavigation.DirectionalNavigation="Contained" KeyboardNavigation.TabIndex="1" Content="{Binding SelectedItem.Content, RelativeSource={RelativeSource TemplatedParent}}" />
							</Grid>
						</Grid>
					</Border>
					<ControlTemplate.Triggers>
						<Trigger Property="ItemsControl.HasItems" Value="False">
							<Setter Property="UIElement.Visibility" Value="Hidden" />
						</Trigger>
						<Trigger Property="ad:Pane.ShowHeader" Value="False">
							<Setter TargetName="PART_Header" Property="UIElement.Visibility" Value="Collapsed" />
						</Trigger>
						<DataTrigger Value="True" Binding="{Binding IsMainDocumentPane, RelativeSource={RelativeSource Self}}">
